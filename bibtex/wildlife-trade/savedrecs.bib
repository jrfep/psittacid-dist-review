@article{ ISI:000489535300001,
Author = {El Bizri, Hani R. and Morcatty, Thais Q. and Valsecchi, Joao and Mayor,
   Pedro and Ribeiro, Jessica E. S. and Vasconcelos Neto, Carlos F. A. and
