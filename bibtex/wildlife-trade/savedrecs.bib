@article{ ISI:000374581200006,
Author = {Costa Lima, Pedro Glecio and Coelho-Ferreira, Marlia and Santos, Ronize
   da Silva},
Title = {{Perspectives on Medicinal Plants in Public Markets across the Amazon: A
   Review}},
