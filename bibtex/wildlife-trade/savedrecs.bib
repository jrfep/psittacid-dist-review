@article{ ISI:000489297100001,
Author = {Berlowitz, Ilana and Walt, Heinrich and Ghasarian, Christian and
   Mendive, Fernando and Martin-Soelch, Chantal},
Title = {{Short-Term Treatment Effects of a Substance Use Disorder Therapy
   Involving Traditional Amazonian Medicine}},
Journal = {{JOURNAL OF PSYCHOACTIVE DRUGS}},
