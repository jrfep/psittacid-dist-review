@article{ ISI:000571082300001,
Author = {Donders, Isabella and Barriocanal, Carles},
Title = {{The Influence of Markets on the Nutrition Transition of
   Hunter-Gatherers: Lessons from the Western Amazon}},
Journal = {{INTERNATIONAL JOURNAL OF ENVIRONMENTAL RESEARCH AND PUBLIC HEALTH}},
Year = {{2020}},
Volume = {{17}},
Number = {{17}},
Month = {{SEP}},
Abstract = {{For many centuries, hunter-gatherer societies relied on subsistence
   practices and traditional diets. However, forces of globalization have
   increased market involvement, thereby fueling the nutrition transition
   of hunter-gatherer societies. We review the academic literature on
   market involvement of hunter-gatherer societies in the Western Amazon
   and its consequences on diet, health and well-being. First, we elaborate
   on four main determinants of market involvement (accessibility, monetary
   income, wild meat trade and social capital), showing how each
   determinant draws individuals toward or away from markets. Thereafter,
   we discuss how these determinants alter diet, health and well-being. Our
   results add to the understanding of the complex relations between market
   involvement, dietary change, health and well-being of indigenous
   societies. Furthermore, they bring to light that additional research is
   needed on the topic to support decision-makers and help preserve
   indigenous values.}},
