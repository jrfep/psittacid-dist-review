@article{ ISI:000340858300006,
Author = {Oubada, A. and Garcia, M. and Bello-Alarcon, A. and Cuesta-Rubio, O. and
   Monzote, L.},
Title = {{Antileishmanial activity of leaf extract from Calophyllum rivulare
   against Leishmania amazonensis}},
Journal = {{EMIRATES JOURNAL OF FOOD AND AGRICULTURE}},
Year = {{2014}},
Volume = {{26}},
Number = {{9}},
Pages = {{807-812}},
Month = {{SEP}},
Abstract = {{Calophyllum rivulare is an endemic tree from Cuba that belongs to
   Clusiaceae family. This plant had revealed it to be a rich source of
   pyranochromanone acids including compounds with a variety of biological
   potentialities such as antioxidant, antiulcer, anticancer and
   anti-Helicobacter pylon activities. Herein, antileishmanial and
   cytotoxic activities of extracts obtained from C. rivulare leaves (rich
   in pyranochromanone acids) and amentoflavone were compared. Extracts
   were prepared by maceration employing ethyl acetate (CR-1) and methanol
   (CR-2) as solvents and amentoflavone was isolated from both extracts.
   The CR-2 extract showed the better activity against the parasite, with
   an IC50 value of 95.1 +/- 5.4 mu g/mL and 20.6 +/- 1.4 mu g/mL against
   promastigote and amastigote forms, respectively. This extract showed
   lower cytotoxicity (IC50=182.4 +/- 2.3 mu g/mL) against peritoneal
   macrophage from BALB/c mice and a selectivity index of 9. Amentoflavone
   showed an IC50 of 5.9 +/- 0.3 mu g/mL against amastigote form and a
   selectivity index of 9. Results suggest that CR-2 extract and
   amentoflavone could be explored as new antileishmanial alternatives.}},
