@article{ ISI:000417533600007,
Author = {de Cristo, Samuel Silva and Baia Junior, Pedro Chaves and da Silva,
   Joilson Silva and Felipe Marques, Jose Ribamar and de Araujo Guimaraes,
   Diva Anelie},
Title = {{The trade of Kinosternon scorpioides on Marajo island, Brazilian Amazon:
   from hunting to consumption.}},
Journal = {{HERPETOLOGICAL JOURNAL}},
