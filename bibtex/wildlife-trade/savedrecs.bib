@article{ ISI:000423297200004,
Author = {Dietrich, Anna-Katharina},
Title = {{Organizing as a trade union strategy to increase associational power. A
   case study of the German United Services Trade Union's (ver.di) campaign
   at Amazon}},
Journal = {{BERLINER JOURNAL FUR SOZIOLOGIE}},
Year = {{2017}},
Volume = {{27}},
Number = {{2}},
Pages = {{243-269}},
Month = {{JUN}},
Abstract = {{Against the background of declining membership and an erosion of power
   resources such as structural and institutional power, German trade
   unions are increasingly turning to the strategy of Organizing as a way
   to bolster their associational power. The article analyses the
   Organizing campaign of the German United Services Trade Union ver.di at
   Amazon's Bad Hersfeld facility, with a view to Organizing's potential
   for strengthening associational power and to the practical challenges
   that its implementation poses in the mail order industry. The one-year
   campaign in 2011 and 2012 led to a significant rise in the rate of
   unionisation and increased the associational power of the Amazon
   employees. Moreover, it led to an improvement of working conditions at
   the ``Fulfillment Center{''} in Bad Hersfeld, despite Amazon's
   continuous refusal to engage in collective bargaining. However, the
   study also shows that the implementation of Organizing as a trade union
   strategy potentially leads to conflicts with different actors such as
   the labor force, the works council, the management or the media, as well
   as causing tension within the trade union itself.}},
