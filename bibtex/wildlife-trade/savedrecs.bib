@article{ ISI:000215775500001,
Author = {Chein Feres, Marcos Vinicio and Freitas Moreira, Joao Vitor},
Title = {{Legal protection of the amazonian biodiversity: the case of traditional
   knowledge}},
Journal = {{REVISTA DIREITO AMBIENTAL E SOCIEDADE}},
Year = {{2014}},
Volume = {{4}},
Number = {{2}},
Pages = {{9-36}},
Month = {{JUL-DEC}},
Abstract = {{The present paper aims to discussing the legality and legitimacy of
   patents related to the traditional knowledge of indigenous communities.
   For this purpose, use will be made of a methodological tool based on the
   theory of Law as identity, conceived as a combination of Dworkin's
   theory of Law as Integrity, the theory of Struggle for Recognition by
   Axel Honneth and Zenon Bankwski's theory of ``living lawfully{''}. Thus,
   stemming from the methodology here applied, the regulation of
   traditional knowledge in international treaties, such as Trips and CBD,
   will be discussed. Furthermore, an empirical study will support the
   research question and the hypothesis and allow us to emphasize the
   indispensability of the protection of traditional knowledge associated
   with indigenous communities. Finally, inferring from the data collected,
   it is possible to affirm that international agreements should be
   reviewed and, specifically, the interpretation of what traditional
   knowledge is should be modified.}},
