@article{ ISI:000561303600006,
Author = {Evangelista, Anderson Pereira and da Silva, Adriane Correa},
Title = {{THE MARUJADA FOLGUEDO PLAYED/DANCED IN ACRE: CARNIVAL TRADITION THAT
   INTEGRATES THE BODY CULTURE OF THE AMAZON STATE}},
Journal = {{HUMANIDADES \& INOVACAO}},
Year = {{2020}},
Volume = {{7}},
Number = {{13}},
Pages = {{59-71}},
Month = {{MAY}},
Abstract = {{The text deals with a study concerning the MARUJADA, a Brazilian group
   from the state of Acre. The objective of the empirical research was to
   describe haw the group is constituted, making on incursion for its
   historicity, through the narratives of a ``playful body{''} and the
   precursor master. The research is configured as qualitative, descriptive
   and field-related. The instruments used to obtain data were: interviews
   and fieid diary. In the discussion, we bring on the folkloric make-up of
   the group into categories of anaysis that arise from the objectives.
   Navigating with the ``Brig Esperance sailors, we found the richness that
   there is in the morujada, as well as its importance for the Amazonian
   body culture and the State of Acre.}},
Publisher = {{FUNDACAO UNIV TOCANTINS}},
Address = {{PRO-REITORIA PESQUISA \& POS-GRADUACAO 108 SUL ALAMEDA 11 LOTE 03 CX
   POSTAL 173, PALMAS-TOCANTINS, CEP77020-122, BRAZIL}},
