@article{ ISI:000420691100006,
Author = {Coelho Paiva, Marco Aurelio},
Title = {{Region, tradition and criticism. Piricles Morais and literary
   representation of Amazon}},
Journal = {{NOVOS CADERNOS NAEA}},
Year = {{2015}},
Volume = {{18}},
Number = {{3}},
Pages = {{101-128}},
Month = {{SEP-DEC}},
Abstract = {{This article attempts to explain how the processes of regional
   representation of the Amazonia were formulated in the years 1920's and
   1930's and how they were directly linked to the new political and
   intellectual configurations in that period. Considering that the
   modernist movement in Brazil had furthered a rearrangement within the
   field of culture, a series of alliances including a range of authors
   from different regions of the country had generated substantial effects
   on the modes of regional representation. From the analysis of some works
   and critical texts of Pericles Moraes, the paper attempts to demonstrate
   how his affirmation as a local author authorized to establish criteria
   of a regionalism Amazonian has rejected the modernist aesthetic
   experimentalism and preserved a literary tradition of understanding
   about the reality of the Amazonia.}},
