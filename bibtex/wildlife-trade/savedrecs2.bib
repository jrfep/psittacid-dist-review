@article{ ISI:A1995QY99400006,
Author = {EARLE, KE and PANNEVIS, MC},
Title = {{ON THE NUTRITIONAL-REQUIREMENTS OF PET BIRDS WITH SPECIAL CONSIDERATION
   OF PSITTACIDAE}},
Journal = {{WIENER TIERARZTLICHE MONATSSCHRIFT}},
Year = {{1995}},
Volume = {{82}},
Number = {{4}},
Pages = {{133-136}},
Abstract = {{The popularity of small psittacine birds as pets varies from country to
   country however, the budgerigar is by far the most common species. Most
   pet birds are fed predominantly on seed-based diets and if
   unsupplemented, these diets can lead to a number of clinical problems.
   Their dietary requirement for most nutrients has not been clearly
   defined, however, some extrapolations can be made from data on poultry
   birds. Formulation of a nutritionally balanced diet using seeds is
   possible if the composition of the constituent seeds is known. An
   understanding of the feeding behaviour of these birds is necessary to
   design a dietary regimen that is not only nutritious but also acceptable
   to the birds. This paper discusses the importance of ensuring the
   adequacy of the key nutrients that are essential to these birds whilst
   not incurring difficulties by overdosing of fat-soluble nutrients.}},
