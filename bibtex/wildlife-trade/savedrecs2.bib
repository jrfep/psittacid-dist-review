@article{ ISI:A1992HA18500001,
Author = {KYLE, SC and CUNHA, AS},
Title = {{NATIONAL FACTOR MARKETS AND THE MACROECONOMIC CONTEXT FOR ENVIRONMENTAL
   DESTRUCTION IN THE BRAZILIAN AMAZON}},
Journal = {{DEVELOPMENT AND CHANGE}},
Year = {{1992}},
Volume = {{23}},
Number = {{1}},
Pages = {{7-33}},
Month = {{JAN}},
Abstract = {{This paper discusses Amazonian agricultural development with emphasis on
   the interrelations between regional conditions and the economic context
   represented by the national economy. The extreme abundance of land
   relative to complementary factors, together with the position of the
   region as a price taker in both factor and output markets, create the
   conditions for environmentally destructive expansion of cultivation at
   the extensive margin. It is argued that policies to promote a more
   ecologically sensitive pattern of development must take into account
   these links with the larger Brazilian economy and society as well as
   conditions within the region itself.}},
