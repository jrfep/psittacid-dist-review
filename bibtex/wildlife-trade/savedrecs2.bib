@article{ ISI:000287891100017,
Author = {Carneiro, Diogo Borges and Leitao Barboza, Myrian Sa and Menezes, Moirah
   Paula},
Title = {{Native plants used by Vila dos Pescadores community, Caete-Taperacu
   Marine Extractive Reserve, Amazon coast, Brazil}},
