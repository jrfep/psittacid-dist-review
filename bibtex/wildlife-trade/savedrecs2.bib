@article{ ISI:000291358100005,
Author = {Kawa, Nicholas C. and Rodrigues, Daniel and Clement, Charles R.},
Title = {{Useful Species Richness, Proportion of Exotic Species, and Market
   Orientation on Amazonian Dark Earths and Oxisols}},
Journal = {{ECONOMIC BOTANY}},
