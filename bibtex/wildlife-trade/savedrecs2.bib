@article{ ISI:000216406500007,
Author = {Bruzaca, Ruan Didier and Costa Sousa, Monica Teresa},
Title = {{FROM MARKET SUPPORT TO ENVIRONMENTAL SUSTAINABILITY: Theories, policies
   and actions in the context of the brazilian Amazon reality}},
Journal = {{VEREDAS DO DIREITO}},
Year = {{2013}},
Volume = {{10}},
Number = {{19}},
Pages = {{133-165}},
Month = {{JAN-JUN}},
Abstract = {{This article intends to analyze the relationship between law and
   development by presenting development as freedom and as a possibility
   for a new meaning, which overcomes the primacy of development as
   economic growth. Therefore, due to the growing understanding of
   sustainable development as a legitimization of certain policies, the
   article will introduce its origins and the transformation of its
   critical content. At last, after describing the reality of the countries
   of peripheral capitalism in Latin America - Brazil in particular - it
   points out perspectives regarding sustainability and development,
   presenting with regards to The Amazon, the impact of a critical legal
   theory, which integrates cultural pluralism and ecological capabilities.}},
