@article{ ISI:000305230700005,
Author = {Newton, Peter and Watkinson, Andrew R. and Peres, Carlos A.},
Title = {{Spatial, Temporal, and Economic Constraints to the Commercial Extraction
   of a Non-timber Forest Product: Copaiba (Copaifera spp.) Oleoresin in
   Amazonian Reserves}},
