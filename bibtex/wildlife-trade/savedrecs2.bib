@article{ ISI:A1997XR00700004,
Author = {Bougerol, C and Peirano, V and Matic, N},
Title = {{Chlamydia psittaci infections in pet birds: a serological and antigenic
   study of a sample of birds}},
Journal = {{RECUEIL DE MEDECINE VETERINAIRE}},
Year = {{1997}},
Volume = {{173}},
Number = {{1-3}},
Pages = {{43-48}},
Month = {{JAN-MAR}},
Abstract = {{In a sample of 174 birds (mainly psittacine birds) screened for avarian
   chlamydiosis, 125 were infected, while only 32 showed symptoms linked to
   the disease. This disease is contagious for man, and is often under
   diagnosed because of the exigence of numerous healthy asymptomatic
   carriers. Serological and antigenic tests can be used to detect
   infection hy avarian chlamydiosis and reveal the presence of chlamydia
   excretors. The same tests performad before and after treatment could be
   used as a guide in the choice of treatment and to give a prognosis of
   the likely evolution of the disease.}},
