@article{ ISI:000074144500020,
Author = {Vitt, LJ and Avila-Pires, TCS and Caldwell, JP and Oliveira, VRL},
Title = {{The impact of individual tree harvesting on thermal environments of
   lizards in Amazonian rain forest}},
Journal = {{CONSERVATION BIOLOGY}},
