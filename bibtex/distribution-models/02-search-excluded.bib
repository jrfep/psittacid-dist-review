Title = {{Follicular choristoma in the third eyelid of an eclectus parrot
   (Eclectus roratus)}},
Journal = {{JAVMA-JOURNAL OF THE AMERICAN VETERINARY MEDICAL ASSOCIATION}},
Year = {{2019}},
Volume = {{255}},
